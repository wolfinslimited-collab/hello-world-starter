{
  "name": "xdrop-api",
  "version": "1.0.0",
  "description": "description",
  "scripts": {
    "dev": "dotenv -e .env.local -- npx prisma db push --schema=prisma/schema.prisma && dotenv -e .env.local -- npx nodemon src/index.ts",
    "start": "npx prisma db push --schema=prisma/schema.prisma && ts-node src/index.ts",
    "sync-dev": "npx prisma db push --schema=prisma/schema.prisma --accept-data-loss",
    "sync-local": "dotenv -e .env.local -- npx prisma db push --schema=prisma/schema.prisma --accept-data-loss",
    "sync-pro": "dotenv -e .env.pro -- npx prisma db push --schema=prisma/schema.prisma --accept-data-loss",
    "build": "tsc && cpx \"src/config/**/*\" dist/config && cpx \"src/MailTemplate/**/*\" dist/MailTemplate",
    "pm2": "npx prisma db push --schema=prisma/schema.prisma && npm run build && pm2 start dist/index.js -n dex-api"
  },
  "author": "",
  "license": "ISC",
